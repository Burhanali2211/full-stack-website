# Supabase Configuration
# Create a new project at https://supabase.com and get these values from your project settings
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Next.js Configuration
# The public URL where your app is hosted
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Authentication Configuration
# The URL where your app is hosted (same as APP_URL for local development)
NEXTAUTH_URL=http://localhost:3000
# Generate a random string for NextAuth.js secret: `openssl rand -base64 32`
NEXTAUTH_SECRET=your-nextauth-secret

# External Services
# Create a new OAuth app at https://github.com/settings/developers
NEXT_PUBLIC_GITHUB_ID=your-github-oauth-client-id
NEXT_PUBLIC_GITHUB_SECRET=your-github-oauth-client-secret 