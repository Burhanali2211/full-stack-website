{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "images": {
    "sizes": [16, 32, 48, 64, 96, 128, 256, 384],
    "domains": [],
    "remotePatterns": []
  },
  "github": {
    "silent": true
  }
}
